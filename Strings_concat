#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int main()
{
   string first_string;
   cout<<"Enter your first_name: "<<endl;
   cin>>first_string;
   
   string last_string;
     cout<<"Enter your last_name: "<<endl;

   cin>>last_string;
   
   string word = first_string + last_string;
   
   std::cout << word << std::endl;
}
