
Problem 01:-

#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int main()
{
   string first_string;
   cout<<"Enter your first_name: "<<endl;
   cin>>first_string;
   
   string last_string;
     cout<<"Enter your last_name: "<<endl;

   cin>>last_string;
   
   string word = first_string + last_string;
   
   std::cout << word << std::endl;
}


Problem 02:-

#include <iostream>
using namespace std;

int main()
{
    int g1,g2,g3,g4,g5;
    cin>>g1>>g2>>g3>>g4>>g5;
    
    
    float avg = (float)(g1+g2+g3+g4+g5)/5;
    cout<<"AVERAGE: "<<avg;
}


Problem 03:-
#include <iostream>
using namespace std;
int main()
{
    int num;
    cin>>num;
    
    int sq = num*num;
    cout<<"Square of num: "<<sq<<endl;
}
