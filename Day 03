Problem 01 :-

#include <iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    
    int sum = 0;
    for(int i = 0;i <= n;i++){
        sum+=i;
    }
    
    cout<<sum;

    return 0;
}



Problem 02:-

#include <iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    
    int fact = 1;
    
    if(n < 0){
        return 0;
    }
    else{
        for(int i = 1; i <= n;i++){
            fact *= i;
        }
    }
    
    cout << "Factorial of n: "<<fact<<endl;




Problem 03:-

#include <iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    
    for(int i = 1; i <= 10;i++){
        cout<< n << "x" << i << "=" << n*i<<endl;
    }
    
    return 0;
}
